<templateSet group="JavaScript">
  <template name="log" value="console.log($END$)" shortcut="TAB" description="console.log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="red" value="reduce((acc, val) =&gt; {&#10;  $action$&#10;  return acc&#10;})" shortcut="ENTER" description="reduce" toReformat="false" toShortenFQNames="true">
    <variable name="action" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="mapr" value="{$array$.map((val, key) =&gt;&#10;  &lt;$tag$ key={key}&gt;&#10;    {val}&#10;  &lt;/$tag$&gt;&#10;)}" description="map" toReformat="false" toShortenFQNames="true">
    <variable name="array" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="tag" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="**" value="{/* $COMMENT$ */}" description="comment" toReformat="false" toShortenFQNames="true">
    <variable name="COMMENT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JSX_HTML" value="true" />
    </context>
  </template>
  <template name="intl" value="intl: { formatMessage }" description="" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ipt" value="import PropTypes from 'prop-types'" shortcut="ENTER" description="PropType" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>